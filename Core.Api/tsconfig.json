{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ES6",
        "moduleResolution": "Node",
        "esModuleInterop": false,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "rootDir": "./src",
        "outDir": "./compiled"
    },
    "exclude": [
        "node_modules",
        "Temp"
    ]
}
